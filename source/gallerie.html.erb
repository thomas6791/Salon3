<h1>Gallerie photos</h1>
<section id="gallerie">
  <div class="container">
    <div class="row"></div>
      <% data.photos.homme.each do |f| %>
      <div class="col-xs-6 col-lg-3">
        <div class="photo-box">
          <%= image_tag f.photo %>
        </div>
        <p class="box-titre"><%= f.titre %></p>
      </div>
      <% end %>
    </div>
  </div>
</section>


